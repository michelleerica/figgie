
<%= form_for @photo do |f| %>

  <%= f.label :image %>
  <%= f.text_field :image %>
  <br>

  <%= f.label :description %>
  <%= f.text_field :description %>
  <br>

  <%= f.label :price_range %>
  <%= f.text_field :price_range %>
  <br>

  <%= f.label :venue_id %>
  <%= f.text_field :venue_id %>
  <br>

  <%= f.label :cuisine_id %>
  <%= f.text_field :cuisine_id %>
  <br>


  <%= f.submit %>

<% end %>
