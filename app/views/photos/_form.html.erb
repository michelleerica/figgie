
<%= form_for @photo do |f| %>

  <%= f.label :image %>
  <%= f.text_field :image %>
  <br>

  <%= f.label :description %>
  <%= f.text_field :description %>
  <br>

  <%= f.label :price_range %>
  <%= f.text_field :price_range %>
  <br>

  <%= f.label :venue_id %>
  <%= f.collection_select( :venue_id, Venue.all, :id, :venue_name, :prompt => "Pick a venue") %> | <%=link_to 'add', new_venue_path(@user_id)%>

  <br>
  <%= f.label :cuisines %> (select cuisines)
  <%#= f.text_field :cuisine.id %>
    <br>
    <%= f.collection_select( :cuisine_ids, Cuisine.all, :id, :category, {include_blank: true }, {:multiple => true}) %>
  <br>


  <%= f.submit %>

<% end %>
