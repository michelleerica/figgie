
<%= form_for @dish, html: {multipart: true} do |f| %>

  <%= f.label "Where did you eat?" %> (select venue)
  <br>
  <%= f.collection_select( :venue_ids, Venue.all, :id, :venue_name) %>
  <br>

  <%= f.label "Image" %>
  <!-- <%#= f.cl_image_upload :image %> -->
<br>
  <%= f.label "Name of dish" %>
  <%= f.text_field :name %>
  <br>
  <%= f.label :cuisines %> (select cuisines)           <%=link_to 'add', new_cuisine_path %>

  <br>
  <%= f.collection_select( :cuisine_ids, Cuisine.all, :id, :category, {include_blank: false }, {:multiple => true}) %>
  <br>

  <%= f.submit %>

<% end %>
