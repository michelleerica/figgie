
<%= form_for @dish, html: {multipart: true} do |f| %>

  <%= f.label :venue_id %>
  <%= f.collection_select( :venue_ids, Venue.all, :id, :venue_name, {include_blank: true }, {:multiple => true}) %> | <%=link_to 'add', new_venue_path(@user_id)%>
  <br>

  <%= f.label :photos.image %>
  <%= f.cl_image_upload :image %>
  <br>

  <%= f.label :photos.description %>
  <%= f.text_field :description %>
  <br>

  <%= f.label :photos.price_range %>
  <%= f.text_field :price_range %>
  <br>

  <br>
  <%= f.label :cuisines %> (select cuisines)
  <%#= f.text_field :cuisine.id %>
  <br>
  <%= f.collection_select( :cuisine_ids, Cuisine.all, :id, :category, {include_blank: true }, {:multiple => true}) %>
  <br>

  <%= f.submit %>

<% end %>
