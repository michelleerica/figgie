
<%= form_for @venue do |f| %>

  <%= f.label :venue_name %>
  <%= f.text_field :venue_name %>
  <br>

  <%= f.label :street_address %>
  <%= f.text_field :street_address %>
  <br>

  <%= f.label :postcode %>
  <%= f.text_field :postcode %>
  <br>

  <%= f.label :website %>
  <%= f.text_field :website %>
  <br>

  <%= f.submit %>

<% end %>
